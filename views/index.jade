extends layout

block content
  h1= title
  p Welcome to #{title}

  div#registry
   != registry
  div#classes
   != classes

  script(src="http://fb.me/react-0.9.0.js")
  script(src="out/goog/base.js", type="text/javascript")
  script(src="out/om_tut.js", type="text/javascript")
  script(type="text/javascript")
    | console.log("About to require om_tut.core");
    | goog.require("om_tut.core");
    | console.log("Done requiring om_tut.core");
    | console.log("registering roots");
    | om_tut.core.root_reg()
    | om_tut.core.root_classes()
    | console.log("done registering roots");
